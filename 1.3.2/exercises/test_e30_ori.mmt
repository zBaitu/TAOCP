[TEST_TARGET e30_ori]

[TEST_SUITE test_e30_ori]

[TEST_BODY]
    LDO     $1,n
    SET     $2,dec
    PUSHJ   $0,:Harmonic
    [ASSERT_STR_EQ actual,expected]
[TEST_BODY_END]

[TEST_CASE n_1]
n               OCTA 1
                LOC (@+7)&-8
dec             GREG @+2
actual          BYTE "  . ",#a,0
expected        BYTE " 3.7",#a,0
[TEST_CASE_END]

